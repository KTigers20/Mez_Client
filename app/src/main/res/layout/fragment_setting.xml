<?xml version="1.0" encoding="utf-8"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    xmlns:app="http://schemas.android.com/apk/res-auto">
    <data>
        <variable
            name="fragment"
            type="com.ktigers20.mez.feature.settings.SettingFragment" />
    </data>
    <FrameLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        tools:context=".feature.settings.SettingFragment">

        <ImageView
            android:id="@+id/logo_img"
            android:layout_width="120dp"
            android:layout_height="18dp"
            android:layout_marginStart="20dp"
            android:layout_marginTop="16dp"
            android:background="#00000000"
            android:src="@drawable/kb_main_logo_img"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintLeft_toLeftOf="parent"/>

        <TextView
            android:layout_marginLeft="165dp"
            android:layout_marginRight="165dp"
            android:layout_marginTop="37dp"
            android:layout_marginBottom="584dp"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:textAlignment="center"
            android:textSize="20sp"
            android:text="설정"
            android:gravity="top"/>
        <TextView
            android:layout_width="112dp"
            android:layout_height="17dp"
            android:layout_marginStart="20dp"
            android:layout_marginBottom="528dp"
            android:layout_marginEnd="228dp"
            android:layout_marginTop="95dp"
            android:text="메인 대시보드 설정"/>

        <FrameLayout
            android:layout_width="140dp"
            android:layout_height="32dp"
            android:layout_marginStart="16dp"
            android:layout_marginTop="119dp"
            android:layout_marginEnd="204dp"
            android:layout_marginBottom="489dp">

            <Button
                android:id="@+id/btn_My"
                android:layout_width="70dp"
                android:layout_height="32dp"
                android:background="#00FF0000"
                android:text="My"/>

            <Button
                android:id="@+id/btn_All"
                android:layout_width="63dp"
                android:layout_height="32dp"
                android:layout_marginStart="80dp"
                android:background="#F2BF41"
                android:text="All" />

        </FrameLayout>

        <TextView
            android:layout_width="92dp"
            android:layout_height="17dp"
            android:layout_marginStart="20dp"
            android:layout_marginBottom="447dp"
            android:layout_marginEnd="248dp"
            android:layout_marginTop="176dp"
            android:text="PUSH알람 설정"/>

        <FrameLayout
            android:layout_marginTop="201dp"
            android:layout_marginStart="20dp"
            android:layout_marginEnd="200dp"
            android:layout_marginBottom="407dp"
            android:layout_width="140dp"
            android:layout_height="32dp">

            <Button
                android:id="@+id/btn_On"
                android:layout_width="70dp"
                android:layout_height="32dp"
                android:background="#00FF0000"
                android:text="On"/>

            <Button
                android:id="@+id/btn_Off"
                android:layout_width="56dp"
                android:layout_height="32dp"
                android:layout_marginStart="80dp"
                android:background="#F2BF41"
                android:text="Off"/>

        </FrameLayout>
        <TextView
            android:layout_width="52dp"
            android:layout_height="17dp"
            android:layout_marginEnd="288dp"
            android:layout_marginBottom="350dp"
            android:layout_marginStart="20dp"
            android:layout_marginTop="273dp"
            android:text="로그아웃"
            android:textColor="@color/kbRed"/>

    </FrameLayout>
</layout>